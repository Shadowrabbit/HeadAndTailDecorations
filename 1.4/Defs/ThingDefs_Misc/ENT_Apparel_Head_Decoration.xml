<Defs>
    <ThingDef ParentName="AFU_ENTHeadDecorationMakeableBase">
        <defName>AFUENT_CatEarsA</defName>
        <label>cat ears(round)</label>
        <graphicData>
            <texPath>Apparel/ENTApparel/CatearsA</texPath>
            <graphicClass>Graphic_Single</graphicClass>
            <drawSize>(0.9, 0.9)</drawSize>
            <shaderType>CutoutComplex</shaderType>
        </graphicData>
        <costList><!--固定材料花费-->
            <Cloth>10</Cloth>
        </costList>
        <statBases>
            <MaxHitPoints>80</MaxHitPoints>
            <WorkToMake>1000</WorkToMake>
            <Mass>0.1</Mass>
            <StuffEffectMultiplierArmor>0</StuffEffectMultiplierArmor>
            <StuffEffectMultiplierInsulation_Cold>0</StuffEffectMultiplierInsulation_Cold>
            <StuffEffectMultiplierInsulation_Heat>0</StuffEffectMultiplierInsulation_Heat>
            <EquipDelay>0.5</EquipDelay><!--穿衣时间-->
        </statBases>
        <apparel>
            <bodyPartGroups><!--身体组-->
                <li>UpperHead</li>
            </bodyPartGroups>
            <wornGraphicPath>Apparel/ENTApparel/CatearsA</wornGraphicPath>
 
            <hatRenderedFrontOfFace>true</hatRenderedFrontOfFace><!--作为面具-->
            <layers><!--穿戴层-->
				<li>AFUHeadDecoration</li>
            </layers>
        </apparel>
        <colorGenerator Class="ColorGenerator_Options">
            <options>
                <li><!--白-->
					 <weight>10</weight>
                     <only>(1,1,1,1)</only>
                </li>
                <li><!--灰-->
					 <weight>10</weight>
                     <only>(0.63,0.62,0.65,1)</only>
                </li>
                <li><!--深灰-->
					 <weight>10</weight>
                     <only>(0.34,0.34,0.35,1)</only>
                </li>
                <li><!--中黄-->
					 <weight>10</weight>
                     <only>(1,0.78,0.43,1)</only>
                </li>
                <li><!--暖红-->
					 <weight>10</weight>
                     <only>(0.75,0.38,0.39,1)</only>
                </li>
                <li><!--浅橙-->
					 <weight>10</weight>
                     <only>(0.89,0.61,0.47,1)</only>
                </li>
                <li><!--浅绿-->
					 <weight>10</weight>
                     <only>(0.76,0.72,0.58,1)</only>
                </li>
            </options>
        </colorGenerator>
    </ThingDef>
