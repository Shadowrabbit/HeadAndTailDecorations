<Defs>
    <!--尾部装饰-->
    <ThingDef ParentName="AFU_ENTTailDecorationMakeableBase">
        <defName>AFUENT_CatTailA</defName>
        <label>条纹猫尾</label>
        <description>猫尾A</description>
        <thingClass>Apparel</thingClass>
        <!--制作材料-->
        <costList>
            <Cloth>10</Cloth>
        </costList>
        <!--渲染数据-->
        <graphicData>
            <texPath>Apparel/CattailA</texPath>
            <graphicClass>Graphic_Single</graphicClass>
            <shaderType>CutoutComplex</shaderType>
        </graphicData>
        <!--基础属性-->
        <statBases>
            <WorkToMake>1000</WorkToMake>
            <Mass>0.1</Mass>
            <EquipDelay>0.5</EquipDelay>
        </statBases>
        <!--服装-->
        <apparel>
            <wornGraphicPath>Apparel/CattailA</wornGraphicPath>
            <careIfWornByCorpse>false</careIfWornByCorpse><!--小人是否介意穿死人的装备-->
            <careIfDamaged>false</careIfDamaged><!--小人是否介意损坏-->
            <wearPerDay>0</wearPerDay>
            <bodyPartGroups>
                <li>Waist</li>
            </bodyPartGroups>
            <layers>
                <li>AFUTailDecoration</li>
            </layers>
        </apparel>
    </ThingDef>
