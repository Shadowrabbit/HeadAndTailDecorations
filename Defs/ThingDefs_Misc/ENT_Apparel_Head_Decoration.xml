<Defs>
    <ThingDef ParentName="AFU_ENTHeadDecorationMakeableBase">
        <defName>AFUENT_CatEarsA</defName>
        <label>圆猫耳</label>
        <description>猫耳A</description>
        <graphicData>
            <texPath>Apparel/CatearsA</texPath>
            <graphicClass>Graphic_Single</graphicClass>
            <drawSize>(0.9, 0.9)</drawSize>
            <shaderType>CutoutComplex</shaderType>
        </graphicData>
        <costList><!--固定材料花费-->
            <Cloth>10</Cloth>
        </costList>
        <statBases>
            <MaxHitPoints>80</MaxHitPoints>
            <WorkToMake>1000</WorkToMake>
            <Mass>0.1</Mass>
            <StuffEffectMultiplierArmor>0</StuffEffectMultiplierArmor>
            <StuffEffectMultiplierInsulation_Cold>0</StuffEffectMultiplierInsulation_Cold>
            <StuffEffectMultiplierInsulation_Heat>0</StuffEffectMultiplierInsulation_Heat>
            <EquipDelay>0.5</EquipDelay><!--穿衣时间-->
        </statBases>
        <apparel>
            <bodyPartGroups><!--身体组-->
                <li>UpperHead</li>
            </bodyPartGroups>
            <wornGraphicPath>Apparel/CatearsA</wornGraphicPath>
            <hatRenderedFrontOfFace>true</hatRenderedFrontOfFace><!--作为面具-->
            <layers><!--穿戴层-->
                <li>AFUHeadDecoration</li>
            </layers>
        </apparel>
        <colorGenerator Class="ColorGenerator_Options">
            <options>
                <li><!--白-->
                    <only>(1,1,1,1)</only>
                </li>
                <li><!--黑-->
                    <only>(0.36,0.36,0.36,1)</only>
                </li>
            </options>
        </colorGenerator>
    </ThingDef>
