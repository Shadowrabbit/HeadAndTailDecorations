<Defs>
    <!--尾部装饰-->
    <ThingDef ParentName="AFU_ENTTailDecorationMakeableBase">
        <defName>AFUENT_CatTailA</defName>
        <label>cat tail(striped)</label>
        <thingClass>Apparel</thingClass>
        <!--制作材料-->
        <costList>
            <Cloth>10</Cloth>
        </costList>
        <!--渲染数据-->
        <graphicData>
            <texPath>Apparel/ENTApparel/CattailA</texPath>
            <graphicClass>Graphic_Single</graphicClass>
            <shaderType>CutoutComplex</shaderType>
        </graphicData>
        <!--基础属性-->
        <statBases>
            <WorkToMake>1000</WorkToMake>
            <Mass>0.1</Mass>
            <EquipDelay>0.5</EquipDelay>
        </statBases>
        <!--服装-->
        <apparel>
            <wornGraphicPath>Apparel/ENTApparel/CattailA</wornGraphicPath>
            <!--这个变量目前并没有什么卵用-->
            <wornGraphicData>
                <renderUtilityAsPack>true</renderUtilityAsPack>
					<north>
						<male>  <offset>(0,-0.15)</offset> </male>
						<female><offset>(0,-0.1)</offset> </female>
						<thin>  <offset>(0,-0.1)</offset> </thin>
						<hulk>  <offset>(0,-0.25)</offset> </hulk>
						<fat>   <offset>(0,-0.25)</offset> </fat>
					</north>
					<east>
						<male>  <offset>(0.05,0)</offset> </male>
						<female><offset>(0,0)</offset> </female>
						<thin>  <offset>(0.05,0)</offset> </thin>
						<hulk>  <offset>(-0.1,-0.2)</offset> </hulk>
						<fat>   <offset>(-0.15,-0.1)</offset> </fat>
					</east>
					<west>
						<male>  <offset>(-0.05,0)</offset> </male>
						<female><offset>(0,0)</offset> </female>
						<thin>  <offset>(-0.05,0)</offset> </thin>
						<hulk>  <offset>(0.1,-0.2)</offset> </hulk>
						<fat>   <offset>(0.15,-0.1)</offset> </fat>
					</west>
            </wornGraphicData>
            <careIfWornByCorpse>false</careIfWornByCorpse><!--小人是否介意穿死人的装备-->
            <careIfDamaged>false</careIfDamaged><!--小人是否介意损坏-->
            <wearPerDay>0</wearPerDay>
            <bodyPartGroups>
                <li>Waist</li>
            </bodyPartGroups>
            <layers>
                <li>AFUTailDecoration</li>
            </layers>
        </apparel>
        <colorGenerator Class="ColorGenerator_Options">
            <options>
                <li><!--白-->
					 <weight>10</weight>
                     <only>(1,1,1,1)</only>
                </li>
                <li><!--灰-->
					 <weight>10</weight>
                     <only>(0.63,0.62,0.65,1)</only>
                </li>
                <li><!--深灰-->
					 <weight>10</weight>
                     <only>(0.34,0.34,0.35,1)</only>
                </li>
                <li><!--灰黄-->
					 <weight>10</weight>
                     <only>(0.76,0.58,0.37,1)</only>
                </li>
                <li><!--亮棕-->
					 <weight>10</weight>
                     <only>(0.72,0.5,0.34,1)</only>
                </li>
                <li><!--灰棕-->
					 <weight>10</weight>
                     <only>(0.65,0.55,0.5,1)</only>
                </li>
                <li><!--粉棕-->
					 <weight>10</weight>
                     <only>(0.71,0.52,0.48,1)</only>
                </li>
            </options>
        </colorGenerator>
    </ThingDef>
